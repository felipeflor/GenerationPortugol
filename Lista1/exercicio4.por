programa {
	funcao inicio() {
	    
	    inteiro a, b, c, r, s
	    real d
	    
	    escreva("Digite o valor de A: ")
	    leia(a)
	    escreva("Digite o valor de B: ")
	    leia(b)
	    escreva("Digite o valor de C: ")
	    leia(c)
	    
	    r = (a+b)*(a+b)
	    s = (b+c)*(b+c)
	    
	    d = (r+s) / 2
	    escreva(d)
	    
	}
}
